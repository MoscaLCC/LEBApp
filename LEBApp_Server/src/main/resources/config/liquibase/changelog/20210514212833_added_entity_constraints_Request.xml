<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Request.
    -->
    <changeSet id="20210514212833-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="dimensions_id"
                                 baseTableName="request"
                                 constraintName="fk_request__dimensions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dimensions"/>

        <addForeignKeyConstraint baseColumnNames="ride_path_id"
                                 baseTableName="request"
                                 constraintName="fk_request__ride_path_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ride_path"/>

        <addForeignKeyConstraint baseColumnNames="producer_id"
                                 baseTableName="request"
                                 constraintName="fk_request__producer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producer"/>

        <addForeignKeyConstraint baseColumnNames="collector_id" 
                                 baseTableName="request" 
                                 constraintName="fk_request__collector_id" 
                                 referencedColumnNames="id" 
                                 referencedTableName="delivery_man"/>

       <addForeignKeyConstraint baseColumnNames="original_point_id"
                                baseTableName="request" 
                                constraintName="fk_request__original_point_id"
                                referencedColumnNames="id" 
                                referencedTableName="point"/>

        <addForeignKeyConstraint baseColumnNames="destination_point_id" 
                                 baseTableName="request" 
                                 constraintName="fk_request__destination_point_id" 
                                 referencedColumnNames="id" 
                                 referencedTableName="point"/>

        <addForeignKeyConstraint baseColumnNames="transporter_id" 
                                 baseTableName="request" 
                                 constraintName="fk_request__transporter_id"
                                 referencedColumnNames="id" 
                                 referencedTableName="transporter"/>

        <addForeignKeyConstraint baseColumnNames="destributor_id" 
                                 baseTableName="request" 
                                 constraintName="fk_request__destributor_id" 
                                 referencedColumnNames="id" 
                                 referencedTableName="delivery_man"/>

    </changeSet>
</databaseChangeLog>
