<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Request.
    -->
    <changeSet id="20210514212833-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="dimensions_id"
                                 baseTableName="request"
                                 constraintName="fk_request__dimensions_id"
                                 referencedColumnNames="id"
                                 referencedTableName="dimensions"/>

        <addForeignKeyConstraint baseColumnNames="ride_path_id"
                                 baseTableName="request"
                                 constraintName="fk_request__ride_path_id"
                                 referencedColumnNames="id"
                                 referencedTableName="ride_path"/>

        <addForeignKeyConstraint baseColumnNames="producer_id"
                                 baseTableName="request"
                                 constraintName="fk_request__producer_id"
                                 referencedColumnNames="id"
                                 referencedTableName="producer"/>
    </changeSet>
</databaseChangeLog>
