<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity Point.
    -->
    <changeSet id="20210514212837-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_info_id"
                                 baseTableName="point"
                                 constraintName="fk_point__user_info_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user_info"/>

        <addForeignKeyConstraint baseColumnNames="zone_id"
                                 baseTableName="point"
                                 constraintName="fk_point__zone_id"
                                 referencedColumnNames="id"
                                 referencedTableName="zone"/>
    </changeSet>
</databaseChangeLog>
